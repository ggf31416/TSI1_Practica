<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Tenant</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bootstrap")
    <!--Reference the SignalR library. -->
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="~/signalr/hubs"></script>
</head>

<div ng-app="tsi" ng-controller="aldeaCtrl">

    @{
        var tenant = ViewBag.Tenant;
    }
    <script type="text/javascript">
        var tenant = '@tenant';
    </script>

    <div id="fb-root"></div>
    
    <script>
(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/es_LA/sdk.js#xfbml=1&version=v2.6&appId=1091835854211637";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>


    <script>
  // This is called with the results from from FB.getLoginStatus().
  function statusChangeCallback(response) {
    // The response object is returned with a status field that lets the
    // app know the current login status of the person.
    // Full docs on the response object can be found in the documentation
    // for FB.getLoginStatus().
    if (response.status === 'connected') {
        // Logged into your app and Facebook.

      testAPI();
    } else if (response.status === 'not_authorized') {
      // The person is logged into Facebook, but not your app.
      document.getElementById('status').innerHTML = 'Please log ' +
        'into this app.';
    } else {
      // The person is not logged into Facebook, so we're not sure if
      // they are logged into this app or not.
      document.getElementById('status').innerHTML = 'Please log ' +
        'into Facebook.';
    }
  }

  // This function is called when someone finishes with the Login
  // Button.  See the onlogin handler attached to it in the sample
  // code below.
  function checkLoginState() {
      FB.getLoginStatus(function (response) {
          console.log('afuera');
         // angular.element($('#login')).scope().loginJuego(response);
    });
  }

  window.fbAsyncInit = function () {
      FB.init({
          appId: '1091835854211637',
          cookie: true,  // enable cookies to allow the server to access
          // the session
          xfbml: true,  // parse social plugins on this page
          version: 'v2.6' // use version 2.2
      });

      // Now that we've initialized the JavaScript SDK, we call
      // FB.getLoginStatus().  This function gets the state of the
      // person visiting this page and can return one of three states to
      // the callback you provide.  They can be:
      //
      // 1. Logged into your app ('connected')
      // 2. Logged into Facebook, but not your app ('not_authorized')
      // 3. Not logged into Facebook and can't tell if they are logged into
      //    your app or not.
      //
      // These three cases are handled in the callback function.

      FB.getLoginStatus(function (response) {
         // angular.element($('#login')).scope().loginJuego(response);
      });
  };
  // Here we run a very simple test of the Graph API after login is
  // successful.  See statusChangeCallback() for when this call is made.
  function testAPI() {
    console.log('Welcome!  Fetching your information.... ');
    FB.api('/me', function(response) {
      console.log('Successful login for: ' + response.name);
      document.getElementById('status').innerHTML =
        'Thanks for logging in, ' + response.name + '!';
    });
  }
    </script>

    <div ng-init="initVariables()" class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="row">
                <div class="col-sm-2 col-md-2 col-lg-2">
                    <button class="btn btn-default" ng-click="listaTecnologias()">Tecnologias</button>
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2">
                    <button class="btn btn-default" ng-click="listaUnidades()">Unidades</button>
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2" ng-if="$root.dataJugador.Clan != null">
                    <button class="btn btn-default" ng-click="abrirClan()">Clanes</button>
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2" ng-if="$root.dataJugador.Clan == null">
                    <button class="btn btn-default" ng-click="abrirCrearClan()">Crear clan</button>
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2">
                    <button class="btn btn-warning" ng-click="listarUsuarios()">Atacar</button>
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2">
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2">
                </div>
                <div class="col-sm-2 col-md-2 col-lg-2"ng-controller="loginCtrl">
                    <div id="login" class="fb-login-button" data-max-rows="1" onlogin="checkLoginState()" data-size="medium" data-show-faces="false" data-auto-logout-link="true"></div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-sm-2 col-md-2 col-lg-2" ng-repeat="rec in $root.dataJugador.EstadoRecursos">
                            <img class="recursosTablero" ng-src="{{getImagenRec(rec.Id)}}"/>{{rec.Total}}
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="row">
                        <div class="col-sm-2 col-md-2 col-lg-2" ng-show="uni.Estado == 0"  ng-repeat="uni in $root.dataJugador.EstadoUnidades">
                            <img class="recursosTablero" ng-src="{{getImagenUnidad(uni.Id)}}"/>{{uni.Cantidad}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-md-12 col-sm-12 col-lg-12 imagenFondo" ng-style="style">
                    <div class="row row-eq-height" ng-repeat="row in $root.tablero.Columnas">
                        <div class="col-xs-1 col-md-1 col-sm-1 col-lg-1 col-centered columnasTablero handPointer" ng-repeat="casilla in row.Fila"
                             style="background-image: url('{{ $root.tablero.ImagenTerreno}}')">
                                <img style="max-height:100%; max-width:100%" class="col-xs-12 col-md-12 col-sm-12 col-lg-12 col-centered columnasTablero handPointer" ng-attr-idCasilla="{{casilla.Id}}" ng-src="{{getImgCasilla(casilla.Id)}}"
                                     ng-click="editCell($parent.$index, $index, casilla)" />
                        </div>
                    </div>
                </div>
            </div>


            <!-- Modal Construir Edificio-->
            <div class="modal fade" id="dialogoConstruir" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Edificio a construir</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <div class="col-sm-2 col-md-2 col-lg-2">Imagen</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Tiempo de construccion</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2" ng-repeat="rec in $root.listaRecursos">
                                            <img style="max-height:100%; max-width:100%" class="recursosTablero" ng-src="{{rec.Imagen}}" />
                                        </div>
                                    </div>
                                    <div class="row handPointer" ng-click="addEdificio(e.Id)" ng-repeat="e in $root.listaEdificios">
                                        <div class="col-sm-2 col-md-2 col-lg-2"><img width="50" height="50" ng-src="{{e.Imagen}}" /></div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.Nombre}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.TiempoConstruccion}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2" ng-repeat="rec in $root.listaRecursos">{{getCostoRecurso(e, rec.Id)}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal Clanes-->
            <div class="modal fade" id="dialogoClanes" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Edificio a construir</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <h3>{{$root.dataJugador.Clan}}  </h3>
                                        <button class="btn btn-danger" ng-click="abandonarClan()">Abandonar Clan</button>
                                    </div>
                                    <div class="row" ng-if="soyAdmin">
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <div class="row">
                                                <h3>Jugadores sin clan</h3>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-2 col-md-2 col-lg-2">Username</div>
                                                <div class="col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                                <div class="col-sm-2 col-md-2 col-lg-2">Apellido</div>
                                            </div>
                                            <div class="row" style="padding:10px" ng-repeat="e in $root.jugadoresSinClan">
                                                <div class="col-sm-2 col-md-2 col-lg-2">{{e.username}}</div>
                                                <div class="col-sm-2 col-md-2 col-lg-2">{{e.nombre}}</div>
                                                <div class="col-sm-2 col-md-2 col-lg-2">{{e.apellido}}</div>
                                                <div class="col-sm-2 col-md-2 col-lg-2"><button class="btn btn-default" ng-click="agregarAlClan(e)">Agregar</button></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <h3>Jugadores en el clan</h3>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-2 col-md-2 col-lg-2">Username</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Apellido</div>
                                    </div>
                                    <div class="row" style="padding:10px" ng-repeat="e in $root.jugadoresEnClan">
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.username}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.nombre}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.apellido}}</div>
                                        <button ng-click="abrirDonarAJugador(e)" class="btn btn-default">Donar recursos</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
            
            <!-- Modal Donar Recursos-->
            <div class="modal fade" id="dialogoDonarRecursos" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Donacion</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <h3>Seleccione recursos para donarle a {{donacion.Nombre}}  </h3>
                                    </div>
                                    <div class="row" ng-repeat="rec in donacion.Valores">
                                        <div class="col-sm-2 col-md-2 col-lg-2">
                                            <img class="recursosTablero" ng-src="{{getImagenRec(rec.IdRecurso)}}" />
                                        </div>
                                        <div class="col-sm-6 col-md-6 col-lg-6">
                                            <input class="form-control" type="text" ng-model="rec.Value" />
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-success" data-dismiss="modal" ng-click="donar()">Donar</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Modal Edificio -->
            <div class="modal fade" id="dialogoDatosEdificio" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Edificio</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row modalRows">
                                <div class="col-sm-3 col-md-3 col-lg-3">
                                    <div class="row">
                                        <h4>Nombre</h4>
                                        <div class="col-sm-9 col-md-9 col-lg-9">
                                            <span class="">{{entidad.Nombre}}</span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <h4>Vida</h4>
                                        <div class="col-md-9">
                                            <span>{{entidad.Vida}}</span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <h4>Ptos. ataque</h4>
                                        <div class="col-md-9">
                                            <span>{{entidad.Ataque}}</span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <h4>Ptos. defensa</h4>
                                        <div class="col-md-9">
                                            <span>{{entidad.Defensa}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-sm-9 col-md-9 col-lg-9">
                                    <div class="row">
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <img style="max-height:100%; max-width:100%" ng-src="{{entidad.Imagen}}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row modalRows" ng-if="unidadesDisponibles.length > 0">
                                <div class="form-group col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <div class="col-sm-3 col-md-3 col-lg-3">
                                            <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                                Crear unidades
                                                <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                                <li style="margin-left:5px;"><b>Unidades</b></li>
                                                <li ng-repeat="e in unidadesDisponibles"><a class="handPointer" ng-click="crearUnidad(e)">{{e.Nombre}}</a></li>
                                            </ul>
                                        </div>
                                        <div class="col-sm-9 col-md-9 col-lg-9">
                                            <div class="row modalRows" ng-repeat="u in unidadesAconstruir">
                                                <div class="col-sm-4 col-md-4 col-lg-4">
                                                    <span>{{u.Nombre}}</span>
                                                </div>
                                                <div class="col-sm-7 col-md-7 col-lg-7">
                                                    <input type="text" class="form-control" ng-model="u.Cantidad" />
                                                </div>
                                                <div class="col-sm-1 col-md-1 col-lg-1">
                                                    <a style="cursor: pointer;" ng-click="removerUnidad(u.Id)">x</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-default" ng-click="entrenarUnidades()" data-dismiss="modal">Save</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal Tecnologias -->
            <div class="modal fade" id="dialogoDatosTecnologias" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Tecnologias</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row" style="padding-top:10px">
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Imagen</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Estado</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Tiempo de construccion</div>
                                        <div class="col-xs-12 col-sm-1 col-md-1 col-lg-1" ng-repeat="rec in $root.listaRecursos">
                                            <img class="recursosTablero" ng-src="{{rec.Imagen}}" />
                                        </div>
                                    </div>
                                    <div class="row handPointer" style="border-top: 1px dotted; padding-top:10px; padding-bottom:10px" ng-click="actualizarTecnologia(e.Id)" ng-repeat="e in $root.listaTecnologias">
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2"><img width="50" height="50" ng-src="{{e.Imagen}}" /></div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{{e.Nombre}}</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-if="getEstadoTecnologia(e) == 3">No es posible</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-if="getEstadoTecnologia(e) == 2">Posible</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-if="getEstadoTecnologia(e) == 1">En desarrollo</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-if="getEstadoTecnologia(e) == 0">Ya desarrollado</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{{e.TiempoConstruccion}}</div>
                                        <div class="col-xs-12 col-sm-1  col-md-1  col-lg-1" ng-repeat="rec in $root.listaRecursos">{{getCostoRecurso(e, rec.Id)}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Modal Unidades -->
            <div class="modal fade" id="dialogoDatosUnidades" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Unidades</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row" style="padding-top:10px">
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Imagen</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">Tiempo de construccion</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-repeat="rec in $root.listaRecursos">
                                            <img class="recursosTablero" ng-src="{{rec.Imagen}}" />
                                        </div>
                                    </div>
                                    <div class="row" style="border-top: 1px dotted; padding-top:10px; padding-bottom:10px" ng-repeat="e in $root.listaUnidades">
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2"><img width="50" height="50" ng-src="{{e.Imagen}}" /></div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{{e.Nombre}}</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2">{{e.TiempoConstruccion}}</div>
                                        <div class="col-xs-12 col-sm-2 col-md-2 col-lg-2" ng-repeat="rec in $root.listaRecursos">{{getCostoRecurso(e, rec.Id)}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Modal Usuarios -->
            <div class="modal fade" id="dialogoDatosUsuarios" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Unidades</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <div class="col-sm-2 col-md-2 col-lg-2">Username</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Nombre</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">Apellido</div>
                                    </div>
                                    <div class="row" style="padding:10px" ng-repeat="e in $root.listaUsuario">
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.username}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.nombre}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2">{{e.apellido}}</div>
                                        <div class="col-sm-2 col-md-2 col-lg-2"><button class="btn btn-danger" ng-click="iniciarAtaque(e.clienteId)">Atacar</button></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Crear Clan -->
            <div class="modal fade" id="dialogoCrearClan" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Datos nuevo clan</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="row">
                                        <h4>Nombre</h4>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" ng-model="NombreClan" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="crearClan()">Save</button>
                        </div>
                    </div>
                </div>
            </div>

            <!--Modal Batalla-->
            <div class="modal fade" id="dialogoBatalla" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title" id="myModalLabel">Batalla</h4>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                    <div class="row">
                                        <h4>{{batalla.msgBatalla}} {{batalla.tiempo}}</h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal" ng-if="batalla.empiezaAtaque" ng-click="iniciarAtaqueFunc()">Close</button>
                            <button type="button" class="btn btn-default" data-dismiss="modal" ng-if="!batalla.empiezaAtaque">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</div>